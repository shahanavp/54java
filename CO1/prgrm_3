class Complex {
    public static void main(String[] args) {
        int a1=5;
        int a2=4;
        int b1=3;
        int b2=2;
        System.out.println("Complex Number 1 : "+a1+" + i"+b1);
        System.out.println("Complex Number 2 : "+a2+" + i"+b2);
        System.out.println("Sum of the Complex Numbers : "+(a1+a2)+" + i"+(b1+b2));
    }
}
